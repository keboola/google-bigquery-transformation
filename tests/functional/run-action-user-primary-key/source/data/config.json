              "CREATE TABLE IF NOT EXISTS `example` (`id` INT64 NOT NULL, `id2` INT64 NOT NULL, `name` STRING, `usercity` STRING, PRIMARY KEY (`id`) NOT ENFORCED);",
              "INSERT INTO `example` (`id`, `id2`, `name`, `usercity`) VALUES (1, 3, 'test example name', 'Prague'), (2, 8, 'test example name 2', 'Brno'), (3, 20, 'test example name 3', 'Ostrava')"
            ]
          }
        ]
      }
    ]
  }
